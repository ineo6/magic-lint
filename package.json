{
  "name": "magic-lint",
  "version": "2.0.0",
  "description": "configless lint tool all in one",
  "keywords": [
    "eslint",
    "husky",
    "lint",
    "lint-staged",
    "prettier",
    "stylelint"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:ineo6/magic-lint.git"
  },
  "license": "MIT",
  "author": "ineo6",
  "bin": "bin/magic-lint.js",
  "scripts": {
    "lint": "node ./bin/magic-lint --eslint bin/ src/",
    "precommit": "node ./bin/magic-lint --staged --eslint --prettier --fix bin/ src/"
  },
  "dependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "common-bin": "2.8.3",
    "debug": "^4.1.1",
    "eslint": "7.12.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-magic": "^2.0.0",
    "eslint-config-prettier": "6.14.0",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.5",
    "globby": "^11.0.0",
    "husky": "4.3.0",
    "ignore": "^5.1.2",
    "lint-staged": "^10.0.8",
    "prettier": "^1.16.4",
    "resolve-bin": "^0.4.0",
    "slash2": "^2.0.0",
    "stylelint": "^13.2.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-order": "^4.0.0",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "7.12.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.5"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
