{
  "name": "magic-lint",
  "version": "1.2.0",
  "description": "configless lint tool all in one",
  "keywords": [
    "eslint",
    "husky",
    "lint",
    "lint-staged",
    "prettier",
    "stylelint"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:ineo6/magic-lint.git"
  },
  "license": "MIT",
  "author": "ineo6",
  "bin": "bin/magic-lint.js",
  "scripts": {
    "lint": "node ./bin/magic-lint --eslint bin/ src/",
    "precommit": "node ./bin/magic-lint --staged --eslint --prettier --fix bin/ src/"
  },
  "dependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "common-bin": "2.8.3",
    "debug": "^4.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.12.4",
    "globby": "^11.0.0",
    "husky": "^4.2.3",
    "ignore": "^5.1.2",
    "lint-staged": "^10.0.8",
    "prettier": "^1.16.4",
    "resolve-bin": "^0.4.0",
    "slash2": "^2.0.0",
    "stylelint": "^13.2.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-order": "^4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "typescript": "^3.5.2"
  },
  "yargs": {
    "camel-case-expansion": false
  }
}
